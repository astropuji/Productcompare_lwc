    "eslint-plugin-import": "^1.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^2.1.6",
    "proxyquire": "1.7.4",
    "sass-loader": "^7.3.1",
    "sgmf-scripts": "^2.3.0",
    "sinon": "^1.17.7",
    "stylelint": "^8.4.0",
